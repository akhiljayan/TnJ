        <meta name="description" content="">

        <title>
            {% block title %}
                Tom & Jerry - Home
            {% endblock %}
        </title>
        {% block stylesheets %}
            <link href="{{ asset('bundles/tomjerry/site/css/style.css') }}" rel="stylesheet" type="text/css">
            <link href="{{ asset('bundles/tomjerry/site/css/shortcodes.css') }}" rel="stylesheet" type="text/css">
            <link href="{{ asset('bundles/tomjerry/site/css/responsive.css') }}" rel="stylesheet" type="text/css">
